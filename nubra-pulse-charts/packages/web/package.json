{
  "name": "@pulse/web",
  "version": "0.1.0",
  "description": "Web renderers for Celeris charts",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "prepare": "bob build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest"
  },
  "keywords": [
    "charting",
    "trading",
    "web"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@pulse/engine": "workspace:*",
    "react": "*"
  },
  "dependencies": {
    "@pulse/engine": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.12",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "react": "19.1.0",
    "react-native-builder-bob": "^0.40.17",
    "typescript": "^5.9.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
